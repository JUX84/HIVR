                            <script language="4">
                                <content>
                                    <![CDATA[from copy import copy
import random

class Info:
    key = ""
    value = ""

    def __init__(self, key, value):
        self.key = key
        self.value = value

    def getKey(self):
        return self.key

    def getValue(self):
        return self.value

class Character:

    def __init__(self, name, sex, color, hair, sport, goodie):
        self.infos = []
        self.infos.append(Info("name", name))
        self.infos.append(Info("sex", sex))
        self.infos.append(Info("color", color))
        self.infos.append(Info("hair", hair))
        self.infos.append(Info("sport", sport))
        self.infos.append(Info("goodie", goodie))

    def getName(self):
        for info in self.infos:
            if info.getKey() == "name":
                return info.getValue()
        return "bug"

    def getInfos(self):
        return self.infos

    def check(self, cmp):
        for info in self.infos:
            if info.getKey() == cmp.getKey():
                if info.getValue() == cmp.getValue().strip():
                    return True
                return False
        return False

class JokeSet:
    category = ""
    jokes = []

    def __init__(self, category, jokes):
        self.category = category
        self.jokes = jokes

    def getCategory(self):
        return self.category

    def getJoke(self):
        if len(self.jokes) > 0:
            return self.jokes.pop(random.randint(0,len(self.jokes)-1))
        return ""

class GuessWho:
    colors = ["vert", "bleu", "jaune", "rouge", "blanc"]
    sex = ["homme", "femme"]
    hair = ["blond", "brun", "roux"]
    sportsM = ["basket", "hockey", "ski", "foot", "golf", "tennis"]
    sportsN = ["equitation"]
    sportsF = ["natation"]
    goodiesM = ["bandeau", "casque", "chapeau", "bonnet", "ballon", "club"]
    goodiesF = ["balle", "chaussure"]
    koTraits = []
    okTraits = []
    miscJokes = ["\\rspd=95\\tu sais que chuck norris a compter jusqu'a l'infini ? \\pau=800\\deux fois.",
            "\\rspd=95\\sais-tu que Google est le seul endroit où tu peux taper Chuck Norris",
            "\\rspd=95\\une légende dit que Chuck Norris donne fréquemment du sang à la Croix-Rouge. Mais jamais le sien.",
            "\\rspd=95\\il parait que les teleteubize sont apparu le lendemain de la rencontre entre les tortues ninja et chuck norris. \\pau=500\\coincidence ? \\pau=500\\je ne pense pas.",
            "\\rspd=95\\Un jour quelqu'un a refuser de filer une clope à Chuck Norris. Depuis, on prévient sur tous les paquets que Fumer tue",
            "\\rspd=95\\Chuck Norris à réussi à trouver la page 404"]
    jokeset = []

    def __init__(self, dialog):
        self.dialog = dialog
        self.status = "idle"
        self.jokeset.append(JokeSet("misc", self.miscJokes))

    def sayJoke(self, category):
        for jokeset in self.jokeset:
            if jokeset.getCategory() == category:
                return jokeset.getJoke()
        return self.jokeset[0].getJoke()


    def generateQuestion(self, trait):
        if self.colors.count(trait) > 0:
            return "est-il habillé de " + trait
        if self.hair.count(trait) > 0:
            return "a-t-il les cheveux " + trait
        if self.sportsM.count(trait) > 0:
            return "fait-il du " + trait
        if self.sportsF.count(trait) > 0:
            return "fait-il de la " + trait
        if self.sportsN.count(trait) > 0:
            return "fait-il de l'" + trait
        if self.goodiesM.count(trait) > 0:
            return "a-t-il un " + trait
        if self.goodiesF.count(trait) > 0:
            return "a-t-il une " + trait
        str = "est-ce "
        if self.sex.count(trait) > 0:
            str += "un"
            if trait == "femme":
                str += "e"
            str += " "
        return str + trait

    def chars(self):
        return self.mesCharacters

    def creationPersonnages(self):
        self.status = "play"
        self.mesCharacters = []
        self.mesCharacters.append(Character("tony", "homme", "vert", "blond", "basket", "ballon"))
        self.mesCharacters.append(Character("max", "homme", "bleu", "und",  "hockey", "casque"))
        self.mesCharacters.append(Character("hugo", "homme", "jaune", "und",  "ski", "casque"))
        self.mesCharacters.append(Character("manon", "femme", "bleu", "brun", "foot", "ballon"))
        self.mesCharacters.append(Character("laura", "femme", "vert", "blond", "basket", "ballon"))
        self.mesCharacters.append(Character("michelle", "femme", "rouge", "brun", "equitation", "chapeau"))
        self.mesCharacters.append(Character("nicolas", "homme", "blanc", "brun", "tennis", "raquette"))
        self.mesCharacters.append(Character("kévin", "homme", "jaune", "brun", "basket", "bandeau"))
        self.mesCharacters.append(Character("pauline", "femme", "bleu", "brun", "natation", "bonnet"))
        self.mesCharacters.append(Character("romain", "homme", "rouge", "roux", "foot", "ballon"))
        self.mesCharacters.append(Character("sarah", "femme", "jaune", "brun", "foot", "bandeau"))
        self.mesCharacters.append(Character("léo", "homme", "bleu", "brun", "golf", "chapeau"))
        #self.mesCharacters.append(Character("thomas", "homme", "rouge", "brun", "hockey", "chaussure"))
        #self.mesCharacters.append(Character("chloé", "femme", "blanc",  "brun", "tennis", "chapeau"))
        #self.mesCharacters.append(Character("julie", "femme", "vert", "blond", "golf", "chapeau"))
        #self.mesCharacters.append(Character("léa", "femme", "jaune", "brun", "volley", "bandeau"))
