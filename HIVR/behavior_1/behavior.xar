                            <bitmap>media/images/box/box-python-script.png</bitmap>
                            <script language="4">
                                <content>
                                    <![CDATA[from copy import copy

class Info:
    key = ""
    value = ""

    def __init__(self, key, value):
        self.key = key
        self.value = value

    def getKey(self):
        return self.key

    def getValue(self):
        return self.value

class Character:
    name = ""
    traits = []

    def __init__(self, name, traits):
        self.name = name
        self.traits = traits

    def getName(self):
        return self.name

    def getTraits(self):
        return self.traits

    def check(self, infos):
        infosArr = []
        i = 0
        while i < len(infos)-1:
            infosArr.append(Info(infos[i], infos[i+1]))
            i += 2
        for info in infosArr:
            if info.getKey() == "name":
                return self.name == info.getValue()
            if info.getKey() == "trait":
                for trait in self.traits:
                    if trait == info.getValue():
                        return True
        return False

class GuessWho:
    character = Character("", [])
    characters = []
    current = []
    availableTraits = []
    okTraits = []
    koTraits = []

    def chars(self):
        return self.characters

    def init(self):
        self.characters.append(Character("tony", ["homme", "caucasien", "blond", "vert", "basket"]))
        self.characters.append(Character("max", ["homme", "caucasien",  "casque", "bleu", "hockey"]))
        self.characters.append(Character("hugo", ["homme", "caucasien", "casque", "jaune", "ski"]))
        self.characters.append(Character("manon", ["femme", "caucasien", "brun", "bleu", "foot"]))
        self.characters.append(Character("laura", ["femme", "caucasien", "blond", "vert", "basket"]))
        self.characters.append(Character("michelle", ["femme", "caucasien", "brun", "chapeau", "rouge", "equitation"]))
        self.characters.append(Character("nicolas", ["homme", "caucasien", "brun", "blanc", "tennis"]))
        self.characters.append(Character("kevin", ["homme", "black", "brun", "bandeau", "jaune", "basket"]))
        self.characters.append(Character("pauline", ["femme", "caucasien", "brun", "bonnet", "bleu", "natation"]))
        self.characters.append(Character("romain", ["homme", "caucasien", "roux", "rouge", "foot"]))
        self.characters.append(Character("sarah", ["femme", "caucasien", "brun", "bandeau", "jaune", "foot"]))
        self.characters.append(Character("leo", ["homme", "black", "brun", "chapeau", "bleu", "golf"]))
        self.characters.append(Character("thomas", ["homme", "black", "brun", "chaussure", "rouge", "hockey"]))
        self.characters.append(Character("clhoe", ["femme", "caucasien", "brun", "chapeau", "blanc", "tennis"]))
        self.characters.append(Character("julie", ["femme", "caucasien", "blonde", "chapeau","chaussure", "vert", "golf"]))
        self.characters.append(Character("lea", ["femme", "caucasien", "brun", "bandeau", "jaune", "volley"]))
        self.characters.append(Character("theo", ["homme", "caucasien", "roux", "bonnet", "bleu", "ski"]))
        self.characters.append(Character("alex", ["homme", "caucasien", "blond", "bandeau", "rouge","balle", "tennis"]))
        self.characters.append(Character("anais", ["femme", "caucasien", "bleu","blanc", "golf"]))
        self.characters.append(Character("mathilde", ["femme", "black", "brun", "chaussure", "rouge", "foot"]))
        self.characters.append(Character("fabio", ["homme", "caucasien", "bleu", "chaussure", "blanc", "foot"]))
        self.characters.append(Character("michael", ["homme", "caucasien", "roux", "rouge", "basket"]))
        self.characters.append(Character("katie", ["femme", "black", "brun", "bonnet", "jaune", "ski"]))
        self.characters.append(Character("marion", ["femme", "caucasien", "roux", "bandeau", "blanc", "basket"]))
    def initA(self):
        self.init()
        self.character = self.characters[0]

    def initB(self):
        self.init()
        self.character = self.characters[11]

    def check(self, infos):
        return self.character.check(infos)

    def refTraits(self):
        self.availableTraits = []
        for character in self.characters:
            for trait in character.getTraits():
                if self.koTraits.count(trait) == 0 and self.okTraits.count(trait) == 0:
                    self.availableTraits.append(trait)

    def ask(self):
        if len(self.characters) == 1:
            self.current = ["name", copy(self.characters[0].getName())]
            return "est-ce "+self.current[1]
        else:
            self.refTraits()
            trait = self.availableTraits.pop(0)
            self.current = ["trait", copy(trait)]
            return "est-il "+self.current[1]

    def process(self, bool):
        if self.current[0] == "name":
            if bool == True:
                return "j'ai gagne"
            else:
                return "j'ai perdu"
        else:
            if bool == False:
                self.koTraits.append(copy(self.current[1]))
            if bool == True:
                self.okTraits.append(copy(self.current[1]))
            tmp = copy(self.characters)
            self.characters = []
            for character in tmp:
                if character.check(self.current) == bool:
                    self.characters.append(copy(character))
            return "ok"

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        global guesswho
        guesswho = GuessWho()]]>
