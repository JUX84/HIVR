        #self.mesCharacters.append(Character("anais", "femme", "bleu", "blanc", "golf", "club"))
        #self.mesCharacters.append(Character("mathilde", "femme", "rouge", "brun", "foot", "chaussure"))
        #self.mesCharacters.append(Character("fabio", "homme", "blanc", "bleu", "foot", "chaussure"))
        #self.mesCharacters.append(Character("michel", "homme", "rouge", "roux", "basket", "ballong"))
        #self.mesCharacters.append(Character("katie", "femme", "jaune", "brun", "ski", "bonnet"))
        #self.mesCharacters.append(Character("marion", "femme", "blanc", "roux", "basket", "bandeau"))
        self.adversaireCharacters = copy(self.mesCharacters)

    def init(self):
        self.status = "play"
        self.creationPersonnages()
        self.character = self.mesCharacters[random.randint(0,len(self.mesCharacters)-1)]
        tmp = []
        self.totalCharacters = float(len(self.mesCharacters))
        for character in self.mesCharacters:
            for info in character.getInfos():
                if info.getKey() != "name" and tmp.count(info.getValue()) == 0:
                    tmp.append(copy(info.getValue()))
        self.totalTraits = float(len(tmp))

    def check(self, infos):
        bool = self.character.check(Info(infos[0], infos[1]))
        if infos[0] == "name":
            if bool == True:
                self.status = "lost"
            else:
                self.status = "won"
        else:
            tmp = copy(self.adversaireCharacters)
            self.adversaireCharacters = []
            for character in tmp:
                if bool == character.check(Info(infos[0], infos[1])):
                    self.adversaireCharacters.append(copy(character))
        return bool

    def refTraits(self):
        self.availableTraits = []
        for character in self.mesCharacters:
            for info in character.getInfos():
                if info.getKey() != "name" and info.getValue() != "" and info.getValue() != "und":
                    b = True
                    for tmp in self.okTraits:
                        if info.getKey() == tmp.getKey():
                            b = False
                    for tmp in self.koTraits:
                        if info.getValue() == tmp.getValue():
                            b = False
                    c = False
                    if b == True:
                        for character in self.mesCharacters:
                            for tmp in character.getInfos():
                                if info.getKey() == tmp.getKey() and info.getValue() != tmp.getValue():
                                    c = True
                    d = True
                    if b == True and c == True:
                        for tmp in self.availableTraits:
                            if info.getKey() == tmp.getKey() and info.getValue() == tmp.getValue():
                                d = False
                    if b == True and c == True and d == True:
                        self.availableTraits.append(info)

    def ask(self):
        if len(self.mesCharacters) == 0:
            self.status = "error"
            return "il y a un probleme"
        if len(self.mesCharacters) == 1:
            self.current = Info("name", copy(self.mesCharacters[0].getName()))
            return "est-ce "+self.current.getValue()
        if self.lose() > 80. and self.win() > 70.:
            self.current = Info("name", copy(self.mesCharacters[random.randint(0,len(self.mesCharacters)-1)].getName()))
            return "est-ce "+self.current.getValue()
        self.refTraits()
        self.current = self.availableTraits.pop(0)
        return self.generateQuestion(self.current.getValue())

    def process(self, bool):
        if self.current.getKey() == "name":
            if bool == True:
                self.status = "won"
            else:
                self.status = "lost"
        else:
            if bool == False:
                self.koTraits.append(copy(self.current))
            if bool == True:
                self.okTraits.append(copy(self.current))
            tmp = copy(self.mesCharacters)
            self.mesCharacters = []
            for character in tmp:
                if character.check(self.current) == bool:
                    self.mesCharacters.append(copy(character))

    def risque(self, tableauPersonnage):
        nombrePersonnage = 0.
        nombreCaracteristiques = 0.
        tmp = []
        present = False

        nombrePersonnage = len(tableauPersonnage)
        for character in tableauPersonnage:
            for info in character.getInfos():
                if info.getKey() != "name" and tmp.count(info.getValue()) == 0:
                    tmp.append(copy(info.getValue()))

        nombreCaracteristiques = len(tmp)
        return (100. -(((nombrePersonnage+nombreCaracteristiques)/(self.totalCharacters+self.totalTraits))*100.))

    def lose(self):
        return self.risque(self.adversaireCharacters)

    def win(self):
        return self.risque(self.mesCharacters)

    def setStatus(self, status):
        self.status = status

    def getStatus(self):
        return self.status


