                if guesswho.getStatus() == "won":
                    self.tts.say("J'ai gagné")
                else:
                    self.tts.say("J'ai perdu")
                time.sleep(3)
                guesswho.setStatus("idle")
                self.over()
        self.onStopped() #activate the output of the box

